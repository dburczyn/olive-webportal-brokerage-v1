{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-bookshelf",
      "settings": {
        "client": "postgres",
        "host": "${process.env.DATABASE_HOST || '127.0.0.1'}",              // IMPORTANT
        "port": "${process.env.DATABASE_PORT || 5432}",                     // IMPORTANT
        "srv": "${process.env.DATABASE_SRV || false}",
        "database": "${process.env.DATABASE_NAME || 'olive-webportal-brokerage-v1'}", // IMPORTANT
        "username": "${process.env.DATABASE_USERNAME || 'postgres'}",                 // IMPORTANT
        "password": "${process.env.DATABASE_PASSWORD || 'postgres'}"                  // IMPORTANT
      },
      "options": {
        "ssl": "${process.env.DATABASE_SSL || false}",
        "authenticationDatabase": "${process.env.DATABASE_AUTHENTICATION_DATABASE || ''}"
      }
    }
  }
}
