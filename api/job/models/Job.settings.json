{
  "connection": "default",
  "collectionName": "jobs",
  "info": {
    "name": "job",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "type": {
      "enum": [
        "Employment",
        "Training",
        "Internship",
        "Master",
        "PhD"
      ],
      "type": "enumeration",
      "default": "Employment"
    },
    "description": {
      "type": "text"
    },
    "email": {
      "type": "email"
    }
  }
}